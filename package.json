{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "An API for a food Ordering website (Fast-Food-Fast).",
  "repository": {
    "type": "git",
    "url": "https://github.com/vkingmaker/fast-food-fast-api.git"
  },
  "author": "Monday Victor Akubudike",
  "license": "MIT",
  "main": "build/src/index.js",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "start": "node build/src/index.js",
    "clean": "rimraf dist",
    "build": "babel src/order.js --out-dir build --presets=es2015 && webpack",
    "test": "mocha",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint",
    "Mocha",
    "Chai",
    "Andela"
  ],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "webpack-cli": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
